{
   "builds":[
       {
           "src":"driving/wsgi.py",
           "use":"@vercel/python",
           "config":{
               "maxLambadaSize":"15mb",
               "runtime":"python3.12"
               }
           },
           {
           "src": "build_files.sh",
           "use": "@vercel/static-build",
           "config": {
             "distDir": "static"
            }
         }
   ],
   "routes":[
       {
           "src":"/(.*)",
           "dest":"driving/wsgi.py"
       }
   ],
   "outputDirectory": "static"
}